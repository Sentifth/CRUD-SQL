<Window x:Class="WPFDatabse.EliminarDatos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFDatabse"
        mc:Ignorable="d"
        Title="EliminarDatos" Height="300" Width="300">
    <Grid>
        <StackPanel Margin="10,26,10,10">
            <TextBox x:Name="txtNombre"
                     Margin="0,0,0,5"/>
            <Button Content="Eliminar"
                    x:Name="btnEliminar"
                    Click="btnEliminar_Click"/>
        </StackPanel>
        <Label Content="Nombre:" FontFamily="Comic Sans MS" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
